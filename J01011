public class Boisochung {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0) {
            long a = sc.nextInt();
            long b = sc.nextInt();
            System.out.println(a*b/gcd(a,b) + " " + gcd(a,b));
        }
    }
    
    public static long gcd(long a, long b) {
        while(a!=0) {
            long x = a;
            a = b % a;
            b = x;
        }
        return b;
    }
}
